<!DOCTYPE html>
<html lang="en">

  <head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Add Sub Catagory</title>

    <!-- Bootstrap core CSS-->
    <link href="http://localhost:8082/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom fonts for this template-->
    <link href="http://localhost:8082/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">

    <!-- Custom styles for this template-->
    <link href="css/sb-admin.css" rel="stylesheet">

  </head>

  <body class="bg-dark">

    <div class="container">
      <div class="card card-register mx-auto mt-5">
        <div class="card-header">Add Sub Catagory</div>
        <div class="card-body">
          <form method="post" name="cat_insert">
            <div class="form-group">
              <div class="form-row">
              	

              	
              	<div class="col-md-12">

				   <div class="form-label-group">
					  <label for="sel1">Catagory List:</label>
					  <select class="form-control" name="catagory_name">
					  	<% for(var i = 0; i < list.length; i++){ %>
					    <option value="<%= list[i].id %>"><%= list[i].cat_name %></option>
					    <% } %>
					  </select>

					</div> 


				  <label for="drop"></label>
                  </div>
                </div>
				



				<div class="col-md-12">
                  <div class="form-label-group">
                    <input type="text" name="scn"  class="form-control" placeholder="Sub Catagory Name" required="required" autofocus="autofocus">
                    <label for="firstName"></label>
                  </div>
                </div>



                
            </div>
            <input type="submit" class="btn btn-primary btn-block" value="Insert">
            <!-- <a class="btn btn-primary btn-block">Insert</a> -->
          </form>
          
        </div>
      </div>
    </div>

    <!-- Bootstrap core JavaScript-->
    <script src="http://localhost:8082/vendor/jquery/jquery.min.js"></script>
    <script src="http://localhost:8082/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="http://localhost:8082/vendor/jquery-easing/jquery.easing.min.js"></script>

  </body>

</html>
